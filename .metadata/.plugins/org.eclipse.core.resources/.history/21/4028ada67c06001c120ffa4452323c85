import java.util.Scanner;

public class sum1 {
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		int t= s.nextInt();
		
		for(int i=1;i<=t;i++) {
			int l=s.nextInt();
			int r= s.nextInt();
			
			int a= r;
			int b=l;
			if(l<=r/2) {
				b=r/2+1;
			}
			
			System.out.println(a+" "+b);
		}
		
	}
}
